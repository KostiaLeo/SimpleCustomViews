<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleView">
        <attr name="circle_style" format="enum">
            <enum name="fill" value="0" />
            <enum name="stroke" value="1" />
            <enum name="fill_and_stroke" value="2" />
        </attr>
        <attr name="circle_radius" format="dimension" />
        <attr name="circle_color" format="color" />
        <attr name="circle_stroke_width" format="dimension" />
    </declare-styleable>

    <declare-styleable name="HotSongView">
        <attr name="song_image" format="string" />
        <attr name="image_src" format="reference" />
        <attr name="song_name" format="string" />
        <attr name="artist_name" format="string" />
    </declare-styleable>
</resources>